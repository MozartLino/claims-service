{
  "name": "claims-service",
  "version": "5.0.0",
  "description": "Batteries-included boilerplate for using AWS CDK v2 with NodejsFunction construct and MiddyJS middleware",
  "scripts": {
    "bootstrap": "npx cdk bootstrap",
    "synth": "npx cdk synth --all",
    "deploy": "npx cdk deploy --all",
    "test": "jest --runInBand=false --maxWorkers=100%",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.886.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/aws-lambda": "^8.10.152",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "aws-cdk": "^2.1029.0",
    "aws-cdk-lib": "^2.214.0",
    "aws-sdk-client-mock": "^4.1.0",
    "constructs": "^10.4.2",
    "esbuild": "0.25.9",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "dependencies": {
    "@aws-lambda-powertools/idempotency": "^2.25.2",
    "@aws-lambda-powertools/logger": "^2.25.2",
    "@aws-sdk/client-dynamodb": "^3.886.0",
    "@aws-sdk/lib-dynamodb": "^3.886.0",
    "@middy/core": "^6.4.5",
    "@middy/http-cors": "^6.4.5",
    "@middy/http-error-handler": "^6.4.5",
    "@middy/http-event-normalizer": "^6.4.5",
    "@middy/http-header-normalizer": "^6.4.5",
    "@middy/http-json-body-parser": "^6.4.5",
    "@middy/http-multipart-body-parser": "^6.4.5",
    "@middy/http-router": "^6.4.5",
    "@middy/http-security-headers": "^6.4.5",
    "@middy/ssm": "^6.4.5",
    "@middy/validator": "^6.4.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "http-errors": "^2.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests --bail --passWithNoTests --coverage=false"
    ]
  }
}
